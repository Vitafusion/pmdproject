simul_vec = function(n=1000,mm){
vec = matrix(0,nrow=100,ncol=mm)
for (i in 1:10) {
for (j in 1:mm) {
if(j==1){
vec[i,j] = sample.int(nn,1)
}
else{
s = nn - sum(vec[i,1:(j-1)])
if(s==0) vec[i,j]=s
else vec[i,j] = sample.int(s,1)
}
}
}
return(vec)
}
